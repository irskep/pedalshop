{
  "name": "pedalz",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "parcel --port=1238 index.html",
    "build": "parcel build index.html --public-url ./",
    "gh": "yarn build && gh-pages -d dist",
    "butler": "yarn build && butler push dist irskep/steves-pedal-shop:dev",
    "pdf": "node pdf.js",
    "deploy": "yarn gh && yarn butler",
    "graph": "pipenv run analysis/graph.py",
    "csv": "pipenv run ./yaml2csv.py",
    "yaml": "pipenv run ./collectyaml.py"
  },
  "dependencies": {
    "@vue/composition-api": "^0.3.2",
    "alea": "^1.0.0",
    "improv": "^1.0.0",
    "number-to-words": "^1.2.4",
    "pluralize": "^8.0.0",
    "query-string": "^6.8.3",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.4"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "gh-pages": "^2.1.1",
    "parcel-bundler": "^1.12.4",
    "puppeteer": "^2.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
